{
  "name": "obsidian-custom-save",
  "version": "1.0.2",
  "description": "Add custom save actions to Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "bun esbuild.config.mjs",
    "build": "bun esbuild.config.mjs production",
    "version": "bun version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "prepare": "husky install",
    "release": "bash ./release.sh"
  },
  "keywords": [
    "obsidian",
    "plugin"
  ],
  "author": "Hananoshika Yomaru",
  "license": "MIT",
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "bun-types": "^1.0.5-canary.20231009T140142",
    "esbuild": "0.17.3",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "observable-slim": "^0.1.6",
    "zod": "^3.22.4"
  }
}
